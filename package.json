{
  "name": "kubernetes-ecr-cleaner",
  "version": "1.0.0",
  "description": "Delete old, unused images from ECR based on your Kubernetes cluster",
  "main": "index.js",
  "bin": {
    "ecr-cleaner": "bin/ecr-cleaner.js"
  },
  "scripts": {
    "test": "xo"
  },
  "keywords": [
    "ecr",
    "ecs",
    "container",
    "registry",
    "container registry",
    "amazon",
    "aws",
    "kubernetes",
    "k8s"
  ],
  "author": "Chris Henderson <cwhenderson20@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async": "2.1.5",
    "aws-sdk": "2.18.0",
    "debug": "2.6.1",
    "moment": "2.17.1",
    "yargs": "6.6.0"
  },
  "devDependencies": {
    "xo": "0.17.1"
  },
  "xo": {
    "rules": {
      "arrow-parens": [2, "always"],
      "no-unused-expressions": [2, { "allowShortCircuit": true }],
      "object-curly-spacing": [2, "always"],
      "comma-dangle": [2, { "objects": "always" }]
    }
  }
}
